
<%= form_with(model: [@artist, @song], local: true, class: "ui form") do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name, required: true, autofocus: true %>
  <br />
  <%= f.label :album %>
  <%= f.text_field :album %>
  <br />
  <%= f.select "billboard_id", options_for_select(@billboards.collect {|u| [u.name, u.id] },{:include_blank => true}) %>
  <br />
  <br />
  <%= f.submit nil, class: "ui button" %>
<% end %>
